{
  "name": "dobitracker-backend",
  "version": "1.0.0",
  "description": "Backend for DobiTracker - Android Notification Capture System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node database/initTables.js",
    "init-admin": "node scripts/initAdmin.js",
    "create-sample-blog": "node scripts/createSampleBlogPost.js",
    "create-sample-docs": "node scripts/createSampleDocuments.js",
    "setup": "npm run init-db && npm run init-admin && npm run create-sample-blog && npm run create-sample-docs",
    "test": "node test-backend.js",
    "demo": "node api-demo.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "axios": "^1.6.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
